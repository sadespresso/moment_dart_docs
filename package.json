{
  "name": "moment-dart-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "prebuild": "cd demo && flutter build web --base-href /moment_dart_docs/demo/ -o ../static/demo && rm ../static/demo/index.html",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "predeploy": "cd demo && flutter build web --base-href /moment_dart_docs/demo/ -o ../static/demo && rm ../static/demo/index.html",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "build:demo": "cd demo && flutter build web --base-href /moment_dart_docs/demo/ -o ../static/demo --source-maps && rm ../static/demo/index.html"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-spinners": "^0.13.8",
    "usehooks-ts": "^2.16.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/types": "^3.7.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "core-js-pure"
    ]
  }
}
